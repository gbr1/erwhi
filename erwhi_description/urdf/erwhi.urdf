<?xml version="1.0"?>

<robot name="erwhi">
	<!-- Base link position -->
	<link name="base_link">
		<visual>
			<!-- <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.06" /> -->
			<origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.06"/>
			<geometry>
				<cylinder length="0.06" radius="0.07" />
			</geometry>
			<material name="blue">
				<color rgba="0 0 1 0.5"/>
			</material>
		</visual>
	</link>
	<!-- Left wheel link -->
	<link name="left_wheel">
		<visual>
			<geometry>
			<!--	<cylinder length="0.01" radius="0.0730250"/> -->
				<cylinder length="0.01" radius="0.0365125"/>
			</geometry>
			<material name="black">
				<color rgba="0 0 0 1"/>
			</material>
		</visual>
    <collision>
      <geometry>
        <!-- <cylinder length="0.01" radius="0.0730250"/> -->
	<cylinder length="0.01" radius="0.0365125"/>
      </geometry>
    </collision>
	</link>
	<!-- Joint configuration left joint -->
	<joint name="left_joint" type="continuous">
		<axis xyz="0 0 -1"/>
		<parent link="base_link"/>
		<child link="left_wheel"/>
		<origin rpy="1.57075 0.0 0.0" xyz="0 0.07 0.0365125"/>
		<limit effort="2" velocity="10"/>
		<joint_properties damping="0.02" friction="0.04"/>
	</joint>
	<!-- Transmission configuration left joint -->
	<transmission name="$left_trans" type="SimpleTransmission">
		<type>transmission_interface/SimpleTransmission</type>
		<actuator name="$left_motor">
			<mechanicalReduction>1</mechanicalReduction>
		</actuator>
		<joint name="left_joint">
			<hardwareInterface>VelocityJointInterface</hardwareInterface>
		</joint>
	</transmission>

	<!-- Right wheel link -->
	<link name="right_wheel">
		<visual>
			<geometry>
			<!--	<cylinder length="0.01" radius="0.0730250"/> -->
				<cylinder length="0.01" radius="0.0365125"/>
			</geometry>
			<material name="black">
				<color rgba="0 0 0 1"/>
			</material>
		</visual>
    <collision>
      <geometry>
        <!-- <cylinder length="0.01" radius="0.0730250"/> -->
	<cylinder length="0.01" radius="0.0365125"/>
      </geometry>
    </collision>
	</link>
	<!-- Joint configuration left joint -->
	<joint name="right_joint" type="continuous">
		<axis xyz="0 0 -1"/>
		<parent link="base_link"/>
		<child link="right_wheel"/>
		<origin rpy="1.57075 0.0 0.0" xyz="0 -0.07 0.0365125"/>
		<limit effort="2" velocity="10"/>
		<joint_properties damping="0.02" friction="0.04"/>
	</joint>
	<!-- Transmission configuration right joint -->
	<transmission name="$right_trans" type="SimpleTransmission">
		<type>transmission_interface/SimpleTransmission</type>
		<actuator name="$right_motor">
			<mechanicalReduction>1</mechanicalReduction>
		</actuator>
		<joint name="right_joint">
			<hardwareInterface>VelocityJointInterface</hardwareInterface>
		</joint>
	</transmission>


<!-- realsense tf -->
	<link name="camera_link">
		<visual>
			<geometry>
				<box size="0.01 0.14 0.02" />
			</geometry>
			<material name="black">
				<color rgba="0 0 0 1"/>
			</material>
		</visual>
		<collision>
			<geometry>
				<box size="0.01 0.14 0.02" />
			</geometry>
		</collision>
	</link>

	<joint name="camera_joint" type="fixed">
		<parent link="base_link"/>
		<child link="camera_link"/>
		<origin rpy="0.0 0.0 0.0" xyz="0.045 0.00 0.094"/>
	</joint>
<!-- -->

<!-- imu tf -->
	<link name="imu_link">
		<visual>
			<geometry>
				<box size="0.02 0.03 0.005" />
			</geometry>
			<material name="black">
				<color rgba="0 0 0 1"/>
			</material>
		</visual>
		<collision>
			<geometry>
				<box size="0.02 0.03 0.005" />
			</geometry>
		</collision>
	</link>

	<joint name="imu_joint" type="fixed">
		<parent link="base_link"/>
		<child link="imu_link"/>
		<origin rpy="0.0 0.0 0.0" xyz="-0.025 0.05 0.11"/>
	</joint>
<!-- -->

<!-- lidar -->
	<link name="base_laser">
	<visual>
		<geometry>
			<cylinder length="0.06" radius="0.035" />
		</geometry>
		<origin xyz="0 0 0.03" />
		<material name="kkk">
			<color rgba="0 0 0 1" />
		</material>
	</visual>
	</link>

	<joint name="base_link_to_base_laser" type="fixed">
		<parent link="base_link"/>
		<child link="base_laser"/>
		<origin xyz="0 0 0.15" rpy="0 0 -1.5708" />
	</joint>
	

		
		
	
	
</robot>
